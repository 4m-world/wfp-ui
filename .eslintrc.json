{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ],
        "wrap-iife": [
            2,
            "inside"
        ],
        "new-cap": [
            2
        ],
        "no-multi-str": [
            2
        ]
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "globals": {
        "$": true
    },
    "extends": "node-style-guide"
}